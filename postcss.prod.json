{
  "use": [
    "postcss-import",
    "postcss-nested",
    "postcss-extend",
    "postcss-custom-properties",
    "postcss-custom-media",
    "postcss-color-function",
    "postcss-focus",
    "autoprefixer",
    "postcss-uncss",
    "cssnano",
    "css-mqpacker"
  ],
  "input": "./css/main.css",
  "output": "./_build/css/main.css",
  "map": {
    "inline": false
  },
  "autoprefixer": {
    "browsers": [
      "last 2 versions",
      "> 5%"
    ]
  },
  "postcss-uncss": {
    "html": "./_build/**/*.html"
  }
}
